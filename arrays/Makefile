.PHONY: clean All

All:
	@echo "----------Building project:[ pos_even_neg_odd - Debug ]----------"
	@cd "pos_even_neg_odd" && "$(MAKE)" -f  "pos_even_neg_odd.mk"
clean:
	@echo "----------Cleaning project:[ pos_even_neg_odd - Debug ]----------"
	@cd "pos_even_neg_odd" && "$(MAKE)" -f  "pos_even_neg_odd.mk" clean
